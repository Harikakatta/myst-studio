{
    "gitbook": "3.2.2",
    "plugins": [
        "hints",
        "simple-page-toc",
        "custom-favicon"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": false
        }, 
        "simple-page-toc": {
            "maxDepth": 7,
            "skipFirstH1": true
        },
        "favicon": "assets/favicon.ico"
    }
}